	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.Objective.Exit3.CFrame
end)

RobberMenu.Esp.Name = "Esp"
RobberMenu.Esp.Parent = RobberMenu._2ndFrame
RobberMenu.Esp.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.Esp.BorderSizePixel = 0
RobberMenu.Esp.Position = UDim2.new(0.704907298, 0, 0.540959001, 0)
RobberMenu.Esp.Size = UDim2.new(0, 117, 0, 32)
RobberMenu.Esp.Font = Enum.Font.SourceSans
RobberMenu.Esp.Text = "Esp"
RobberMenu.Esp.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.Esp.TextSize = 14.000
RobberMenu.Esp.MouseButton1Down:connect(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = "Robber Menu";
		Text = "NumPad 1 Show/Hide";
		Duration = 3;
	})
	local custom_theme = {} 

	local function CreateInstance(cls,props)
		local inst = Instance.new(cls)
		for i,v in pairs(props) do
			inst[i] = v
		end
		return inst
	end

	local age1 = CreateInstance('ScreenGui',{DisplayOrder=0,Enabled=true,ResetOnSpawn=true,Name='age1', Parent=game.CoreGui})
