LoadWeapons.Name = "Load Weapons"
LoadWeapons.Parent = WeaponsMods_2
LoadWeapons.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
LoadWeapons.BorderSizePixel = 0
LoadWeapons.Position = UDim2.new(0.0388413966, 0, 0.898128867, 0)
LoadWeapons.Size = UDim2.new(0, 152, 0, 30)
LoadWeapons.Font = Enum.Font.SourceSans
LoadWeapons.Text = "Click Me First"
LoadWeapons.TextColor3 = Color3.fromRGB(255, 255, 255)
LoadWeapons.TextSize = 28.000
LoadWeapons.MouseButton1Down:connect(function()
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons["Beretta M9-S"];
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons.M1911;
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons.MP5;
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons["MAC-10"];
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons.M4A1;
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons.Flashlight;
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons.Knife;
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons["Desert Eagle"];
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons["Five-seveN"];
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets:Clone().Parent = game:GetService("ReplicatedStorage").Weapons.G17;
	bc = BrickColor.new("White")
	game.StarterGui:SetCore("ChatMakeSystemMessage", {
		Text = "[Mrgunz#2289]: Loaded Bullets.";
		Font = Enum.Font.SourceSans;
		Color = bc.Color;
		FontSize = Enum.FontSize.Size9;
	})
end)

warn("Loading RGB Part Two")
local function XISX_fake_script() -- UIv4.RGB 
	local script = Instance.new('LocalScript', UIv4)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.00021
	end
end
coroutine.wrap(XISX_fake_script)()
local function TLVTGZ_fake_script() -- UIv3.RGB 
	local script = Instance.new('LocalScript', UIv3)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.00021
	end
end
coroutine.wrap(TLVTGZ_fake_script)()
local function DNXY_fake_script() -- UIv2.RGB 
	local script = Instance.new('LocalScript', UIv2)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.00021
	end
end
coroutine.wrap(DNXY_fake_script)()
