			_G.Value = RobberMenu.BerettaM9SAmmoTextBox.Text
			game:GetService("ReplicatedStorage").Weapons["Beretta M9-S"].Ammo.Value = _G.Value
			game:GetService("ReplicatedStorage").Weapons["Beretta M9-S"].StoredAmmo.Value = _G.Value
		end
		if on == false then
			button.Text = "Ammo (False)"
			game:GetService("ReplicatedStorage").Weapons["Beretta M9-S"].Ammo.Value = 15
			game:GetService("ReplicatedStorage").Weapons["Beretta M9-S"].StoredAmmo.Value = 45
		end
	end)
end)

