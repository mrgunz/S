warn("Loading RGB.")
local function RIFEA_fake_script() -- RobberMenu.UIv3.RGB 
	local script = Instance.new('LocalScript', RobberMenu.UIv3)

	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end

	counter = 0

	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)

		counter = counter + 0.00021
	end
end
coroutine.wrap(RIFEA_fake_script)()
