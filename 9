	game:GetService("ReplicatedStorage").Weapons.G17.FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Five-seveN"].FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Desert Eagle"].FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons.M4A1.FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["MAC-10"].FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Beretta M9-S"].FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons.MP5.FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons.M1911.FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons.Flashlight.FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons.Knife.FireRate.Value = 0.0001
	---- Hit Frame ----
	game:GetService("ReplicatedStorage").Weapons.Flashlight.HitFrame.Value = 1e+14
	game:GetService("ReplicatedStorage").Weapons.Knife.HitFrame.Value = 1e+14
	print("Fire Rate And Hit Frame Modded.")
end)


