warn("Finding Robber Menu")
for i,v in pairs(game:GetService("CoreGui"):GetChildren()) do
	if v.Name == "Robber Menu" then
		v:Destroy()
		warn ("Robber Menu Removed.")
	end
end
warn("Made By Mrgunz#2289")
game.StarterGui:SetCore("SendNotification", {
	Title = "Robber Menu Loading";
	Text = "Made By Mrgunz#2289";
	Duration = 5;
})


