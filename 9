RobberMenu.FullAuto.Name = "Full Auto"
RobberMenu.FullAuto.Parent = RobberMenu.GunMenuFrame
RobberMenu.FullAuto.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.FullAuto.BorderSizePixel = 0
RobberMenu.FullAuto.Position = UDim2.new(0.366523921, 0, 0.585499585, 0)
RobberMenu.FullAuto.Size = UDim2.new(0, 117, 0, 30)
RobberMenu.FullAuto.Font = Enum.Font.SourceSans
RobberMenu.FullAuto.Text = "Full Auto"
RobberMenu.FullAuto.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.FullAuto.TextSize = 16.000
RobberMenu.FullAuto.MouseButton1Down:connect(function()
	game:GetService("ReplicatedStorage").Weapons.G17.Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons["Five-seveN"].Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons["Desert Eagle"].Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons.M4A1.Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons["MAC-10"].Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons["Beretta M9-S"].Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons.MP5.Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons.M1911.Auto.Value = true
	print("All weapons are full auto.")
end)

