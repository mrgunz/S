RobberMenu.UIv3_2.Name = "UI v3"
RobberMenu.UIv3_2.Parent = RobberMenu.MainMenu
RobberMenu.UIv3_2.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
RobberMenu.UIv3_2.BorderSizePixel = 0
RobberMenu.UIv3_2.Position = UDim2.new(0, 0, 0.99065423, 0)
RobberMenu.UIv3_2.Size = UDim2.new(0, 477, 0, 3)
