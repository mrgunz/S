loadstring(game:HttpGet("https://raw.githubusercontent.com/mrgunz/S/main/List"))();
wait(2)
game.StarterGui:SetCore("SendNotification", {
	Title = "Robber Menu Loaded.";
	Text = "Made By Mrgunz#2289";
	Duration = 30;
})
bc = BrickColor.new("White")
game.StarterGui:SetCore("ChatMakeSystemMessage", {
	Text = "[Mrgunz#2289]: P Hide/Show";
	Font = Enum.Font.SourceSans;
	Color = bc.Color;
	FontSize = Enum.FontSize.Size9;
})
bc = BrickColor.new("White")
game.StarterGui:SetCore("ChatMakeSystemMessage", {
	Text = "[Mrgunz#2289]: Last Update.";
	Font = Enum.Font.SourceSans;
	Color = bc.Color;
	FontSize = Enum.FontSize.Size9;
})
RobberMenu.RobberMenu.Name = "Robber Menu"
RobberMenu.RobberMenu.Parent = game.CoreGui
game:GetService("UserInputService").InputBegan:connect(function(input, gameProcessed)
	if input.KeyCode == Enum.KeyCode.P then
		if not gameProcessed then
			RobberMenu.RobberMenu.Enabled = not RobberMenu.RobberMenu.Enabled
		end
	end
end)
RobberMenu.Settings.Name = "Settings"
