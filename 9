RobberMenu.UIv4_3.Name = "UI v4"
RobberMenu.UIv4_3.Parent = RobberMenu.MainMenu
RobberMenu.UIv4_3.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
RobberMenu.UIv4_3.BorderSizePixel = 0
RobberMenu.UIv4_3.Position = UDim2.new(0.993710697, 0, 0.00311512221, 0)
RobberMenu.UIv4_3.Size = UDim2.new(0, 3, 0, 320)
RobberMenu.UIv4_3.Font = Enum.Font.SourceSans
RobberMenu.UIv4_3.Text = ""
