RobberMenuv4.Name = "Robber Menu v4"
RobberMenuv4.Parent = game.CoreGui

MainMenu.Name = "Main Menu"
MainMenu.Parent = RobberMenuv4
MainMenu.Active = true
MainMenu.BackgroundColor3 = Color3.fromRGB(40, 40, 40)
MainMenu.BorderSizePixel = 0
MainMenu.Position = UDim2.new(0.00672782958, 0, 0.371359229, 0)
MainMenu.Size = UDim2.new(0, 477, 0, 321)
MainMenu.Draggable = true
game:GetService("UserInputService").InputBegan:connect(function(input, gameProcessed)
	if input.KeyCode == Enum.KeyCode.P then
		if not gameProcessed then
			RobberMenuv4.Enabled = not RobberMenuv4.Enabled
		end
	end
end)


