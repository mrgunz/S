RobberMenu.SuperShotgun.Size = UDim2.new(0, 117, 0, 30)
RobberMenu.SuperShotgun.Font = Enum.Font.SourceSans
RobberMenu.SuperShotgun.Text = "Super Shotgun"
RobberMenu.SuperShotgun.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.SuperShotgun.TextSize = 16.000
RobberMenu.SuperShotgun.MouseButton1Down:connect(function()
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Bullets.Value = 13
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].EndReload.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].AfterReload.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].StartReload.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Auto.Value = true
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].FireRate.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].MSX.Value = 0.0001
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].Ammo.Value = 99999999
	game:GetService("ReplicatedStorage").Weapons["Ithaca 37"].PumpTime.Value = 0.0001
	print("Ithaca 37 Now Shoots 50 Bullets.")
	print("Ithaca 37 Modded.")
end)

