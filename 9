RobberMenu.DesertEagleRangeButton.Name = "Desert Eagle Range Button"
RobberMenu.DesertEagleRangeButton.Parent = RobberMenu.DesertEagle
RobberMenu.DesertEagleRangeButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.DesertEagleRangeButton.BorderSizePixel = 0
RobberMenu.DesertEagleRangeButton.Position = UDim2.new(0.246184871, 0, 0.260572344, 0)
RobberMenu.DesertEagleRangeButton.Size = UDim2.new(0, 118, 0, 21)
RobberMenu.DesertEagleRangeButton.Font = Enum.Font.SourceSans
RobberMenu.DesertEagleRangeButton.Text = "Range (False)"
RobberMenu.DesertEagleRangeButton.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.DesertEagleRangeButton.TextSize = 14.000
RobberMenu.DesertEagleRangeButton.MouseButton1Down:connect(function()
	local button = RobberMenu.DesertEagleRangeButton
	local on = false

	button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			button.Text = "Range (True)"
			print("Desert Eagle Range Button True.")
			_G.Value = RobberMenu.DesertEagleRangeTextBox.Text
			game:GetService("ReplicatedStorage").Weapons["Desert Eagle"].RangeModifier.Value = _G.Value
			game:GetService("ReplicatedStorage").Weapons["Desert Eagle"].Range.Value = _G.Value
			print ("Desert Eagle Range Modded.")
		end
		if on == false then
			button.Text = "Range (False)"
			print("Desert Eagle Range Button Fasle.")
			game:GetService("ReplicatedStorage").Weapons["Desert Eagle"].RangeModifier.Value = 75
			game:GetService("ReplicatedStorage").Weapons["Desert Eagle"].Range.Value = 450
		end
	end)
end)

