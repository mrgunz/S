BreakServer.Name = "Break Server"
BreakServer.Parent = _2ndFrame
BreakServer.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
BreakServer.BorderSizePixel = 0
BreakServer.Position = UDim2.new(0.377358496, 0, 0.228666648, 0)
BreakServer.Size = UDim2.new(0, 117, 0, 32)
BreakServer.Font = Enum.Font.SourceSans
BreakServer.Text = "Break Server"
BreakServer.TextColor3 = Color3.fromRGB(0, 0, 0)
BreakServer.TextSize = 14.000
BreakServer.MouseButton1Down:connect(function()
	print "Started"
	game.StarterGui:SetCore("SendNotification", 
		{Title = "Robber Cheat";
			Text = "Breaking Server";
			Duration = 3;})
	wait(1)
	local LocalPlayer = game:GetService("Players").LocalPlayer
	local char = Instance.new("Model", workspace)

	LocalPlayer.Character:ClearAllChildren()
	char.Name = "character"
	Instance.new("Part", char).Name = "Torso";
	Instance.new("Part", char).Name = "Head";
	Instance.new("Humanoid", char).Name = "Humanoid";
	LocalPlayer.Character = char

	wait(2)
	print "Done"
	game.StarterGui:SetCore("SendNotification", 
		{Title = "Robber Cheat";
			Text = "Done";
			Duration = 3;})
end)


ChatSpy.Name = "Chat Spy"
ChatSpy.Parent = _2ndFrame
ChatSpy.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
ChatSpy.BorderSizePixel = 0
ChatSpy.Position = UDim2.new(0.0503144674, 0, 0.228666648, 0)
ChatSpy.Size = UDim2.new(0, 117, 0, 32)
ChatSpy.Font = Enum.Font.SourceSans
ChatSpy.Text = "Chat Spy (Buggy)"
ChatSpy.TextColor3 = Color3.fromRGB(0, 0, 0)
ChatSpy.TextSize = 14.000
ChatSpy.MouseButton1Down:connect(function()
	local players = game:GetService("Players");
	local repStorage = game:GetService("ReplicatedStorage");

	setreadonly(string, false)
	string.startswith = function(self, stuff)
		local found = self:find("^" .. stuff) ~= nil;

		return found;
	end;

	local say = function(...)
		repStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(..., "All");
	end;

	for _, v in next, players:GetPlayers() do
		v.Chatted:Connect(function(msg)
			if msg:startswith("/t") then
				say("Spy BOT");
				say("[" .. v.Name .. "]: " .. msg);
			end;
		end);
	end;

	local players = game:GetService("Players");
	local repStorage = game:GetService("ReplicatedStorage");

	setreadonly(string, false)
	string.startswith = function(self, stuff)
		local found = self:find("^" .. stuff) ~= nil;

		return found;
	end;

	local say = function(...)
		repStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(..., "All");
	end;

	for _, v in next, players:GetPlayers() do
		v.Chatted:Connect(function(msg)
			if msg:startswith("/w") then
				say("Spy BOT");
				say("[" .. v.Name .. "]: " .. msg);
			end;
		end);
	end;

	local players = game:GetService("Players");
	local repStorage = game:GetService("ReplicatedStorage");

	setreadonly(string, false)
	string.startswith = function(self, stuff)
		local found = self:find("^" .. stuff) ~= nil;

		return found;
	end;

	local say = function(...)
		repStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(..., "All");
	end;

	for _, v in next, players:GetPlayers() do
		v.Chatted:Connect(function(msg)
			if msg:startswith("/me") then
				say("Spy BOT");
				say("[" .. v.Name .. "]: " .. msg);
			end;
		end);
	end;
end)

