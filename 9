RobberMenu.M4A1.Name = "M4A1"
RobberMenu.M4A1.Parent = RobberMenu.WeaponsScrollingFrame

RobberMenu.M4A1AlwaysAutoButton.Name = "M4A1 Always Auto Button"
RobberMenu.M4A1AlwaysAutoButton.Parent = RobberMenu.M4A1
RobberMenu.M4A1AlwaysAutoButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.M4A1AlwaysAutoButton.BorderSizePixel = 0
RobberMenu.M4A1AlwaysAutoButton.Position = UDim2.new(0.24415648, 0, 0.34176892, 0)
RobberMenu.M4A1AlwaysAutoButton.Size = UDim2.new(0, 118, 0, 21)
RobberMenu.M4A1AlwaysAutoButton.Font = Enum.Font.SourceSans
RobberMenu.M4A1AlwaysAutoButton.Text = "Always Auto (False)"
RobberMenu.M4A1AlwaysAutoButton.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.M4A1AlwaysAutoButton.TextSize = 14.000
RobberMenu.M4A1AlwaysAutoButton.MouseButton1Down:connect(function()
	local button = RobberMenu.M4A1AlwaysAutoButton
	local on = false

	button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			button.Text = "Always Auto (True)"
			_G.Value = RobberMenu.M4A1AlwaysAutoTextBox.Text
			game:GetService("ReplicatedStorage").Weapons.M4A1.Auto.Value = _G.Value
		end
		if on == false then
			button.Text = "Always Auto (False)"
			game:GetService("ReplicatedStorage").Weapons.M4A1.Auto.Value = true
		end
	end)
end)

RobberMenu.M4A1AmmoButton.Name = "M4A1 Ammo Button"
RobberMenu.M4A1AmmoButton.Parent = RobberMenu.M4A1
RobberMenu.M4A1AmmoButton.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.M4A1AmmoButton.BorderSizePixel = 0
RobberMenu.M4A1AmmoButton.Position = UDim2.new(0.24415648, 0, 0.38225472, 0)
RobberMenu.M4A1AmmoButton.Size = UDim2.new(0, 118, 0, 21)
RobberMenu.M4A1AmmoButton.Font = Enum.Font.SourceSans
RobberMenu.M4A1AmmoButton.Text = "Ammo (False)"
RobberMenu.M4A1AmmoButton.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.M4A1AmmoButton.TextSize = 14.000
RobberMenu.M4A1AmmoButton.MouseButton1Down:connect(function()
	local button = RobberMenu.M4A1AmmoButton
	local on = false

	button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			button.Text = "Ammo (True)"
			_G.Value = RobberMenu.M4A1AmmoTextBox.Text
			game:GetService("ReplicatedStorage").Weapons.M4A1.Ammo.Value = _G.Value
			game:GetService("ReplicatedStorage").Weapons.M4A1.StoredAmmo = _G.Value
		end
		if on == false then
			button.Text = "Ammo (False)"
			game:GetService("ReplicatedStorage").Weapons.M4A1.Ammo.Value = 30
			game:GetService("ReplicatedStorage").Weapons.M4A1.StoredAmmo.Value = 90
		end
	end)
end)

