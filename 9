RobberMenu.PickUpAllGuns.Name = "Pick Up All Guns"
RobberMenu.PickUpAllGuns.Parent = RobberMenu.GunMenuFrame
RobberMenu.PickUpAllGuns.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.PickUpAllGuns.BorderSizePixel = 0
RobberMenu.PickUpAllGuns.Position = UDim2.new(0.0351494141, 0, 0.582166195, 0)
RobberMenu.PickUpAllGuns.Size = UDim2.new(0, 117, 0, 32)
RobberMenu.PickUpAllGuns.Font = Enum.Font.SourceSans
RobberMenu.PickUpAllGuns.Text = "Pick Up All Guns"
RobberMenu.PickUpAllGuns.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.PickUpAllGuns.TextSize = 16.000
RobberMenu.PickUpAllGuns.MouseButton1Down:connect(function()
	local args = {
		[1] = {
			[1] = "Pickup",
			[2] = workspace.Map.Ignore.M4A1
		}
	}

	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))

	wait(1)

	local args = {
		[1] = {
			[1] = "Pickup",
			[2] = workspace.Map.Ignore["Ithaca 37"]
		}
	}

	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))

	wait(1)

	local args = {
		[1] = {
			[1] = "Pickup",
			[2] = workspace.Map.Ignore["MAC-10"]
		}
	}

	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))

	wait(1)

	local args = {
		[1] = {
			[1] = "Pickup",
			[2] = workspace.Map.Ignore["MAC-10"]
		}
	}

	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))

	wait(1)

	local args = {
		[1] = {
			[1] = "Pickup",
			[2] = workspace.Map.Ignore["Ithaca 37"]
		}
	}

	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))

	wait(1)

	local args = {
		[1] = {
			[1] = "Pickup",
			[2] = workspace.Map.Ignore.M4A1
		}
	}

	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))
end)

