RobberMenu.DoObjective.MouseButton1Down:connect(function()
	game.StarterGui:SetCore("SendNotification", {
		Title = "Robber Menu";
		Text = "Robber Team Only";
		Duration = 3;
	})
	print "Started"
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.Objective.Ruby.CFrame
	wait()
	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = game.Workspace.Map.Objective.Emerald.CFrame
	---- Computers ----
	local args = {
		[1] = {
			[1] = "hackputer",
			[2] = workspace.Map.Objective.Computers.Computer1
		}
	}
	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))

	local args = {
		[1] = {
			[1] = "hackputer",
			[2] = workspace.Map.Objective.Computers.Computer2
		}
	}
	game:GetService("ReplicatedStorage").Events.RemoteEvent:FireServer(unpack(args))
	local args = {
		[1] = {
			[1] = "hackputer",
			[2] = workspace.Map.Objective.Computers.Computer3
		}
