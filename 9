RobberMenu.SuperShotgun.Name = "Super Shotgun"
RobberMenu.SuperShotgun.Parent = RobberMenu.GunMenuFrame
RobberMenu.SuperShotgun.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.SuperShotgun.BorderSizePixel = 0
