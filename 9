	game:GetService("ReplicatedStorage").Weapons.M1911.RangeModifier.Value = 1e+14
	game:GetService("ReplicatedStorage").Weapons.Flashlight.RangeModifier.Value = 1e+14
	game:GetService("ReplicatedStorage").Weapons.Knife.RangeModifier.Value = 1e+14
	---- Range ----
	game:GetService("ReplicatedStorage").Weapons.G17.Range.Value = 1e+14
	game:GetService("ReplicatedStorage").Weapons["Five-seveN"].Range.Value = 1e+14
	game:GetService("ReplicatedStorage").Weapons["Desert Eagle"].Range.Value = 1e+14
