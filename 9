RobberMenu.FiveSevenRangeButton.BorderSizePixel = 0
RobberMenu.FiveSevenRangeButton.Position = UDim2.new(0.246184871, 0, 0.151710451, 0)
RobberMenu.FiveSevenRangeButton.Size = UDim2.new(0, 118, 0, 21)
RobberMenu.FiveSevenRangeButton.Font = Enum.Font.SourceSans
RobberMenu.FiveSevenRangeButton.Text = "Range (False)"
RobberMenu.FiveSevenRangeButton.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.FiveSevenRangeButton.TextSize = 14.000
RobberMenu.FiveSevenRangeButton.MouseButton1Down:connect(function()
	local button = RobberMenu.FiveSevenRangeButton
	local on = false

	button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			button.Text = "Range (True)"
			_G.Value = RobberMenu.FiveSevenRangeTextBox.Text
			game:GetService("ReplicatedStorage").Weapons.G17.RangeModifier.Value = _G.Value
			game:GetService("ReplicatedStorage").Weapons["Five-seveN"].Range.Value = _G.Value
		end
		if on == false then
			button.Text = "Range (False)"
			game:GetService("ReplicatedStorage").Weapons.G17.RangeModifier.Value = 80
			game:GetService("ReplicatedStorage").Weapons["Five-seveN"].Range.Value = 400
		end
	end)
end)

