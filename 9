RobberMenu.DeleteMenuButton.MouseButton1Down:connect(function()
	bc = BrickColor.new("White")
	game.StarterGui:SetCore("ChatMakeSystemMessage", {
		Text = "Removing Robber Menu";
		Font = Enum.Font.SourceSans;
		Color = bc.Color;
		FontSize = Enum.FontSize.Size9;
	})
	game:GetService("CoreGui")["Robber Menu"]:Destroy()
end)

