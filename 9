RobberMenu.M4A1RangeButton.Text = "Range (False)"
RobberMenu.M4A1RangeButton.TextColor3 = Color3.fromRGB(0, 0, 0)
RobberMenu.M4A1RangeButton.TextSize = 14.000
RobberMenu.M4A1RangeButton.MouseButton1Down:connect(function()
	local button = RobberMenu.M4A1RangeButton
	local on = false

	button.MouseButton1Click:Connect(function()
		on = not on
		if on then
			button.Text = "Range (True)"
			_G.Value = RobberMenu.M4A1RangeTextBox.Text
			game:GetService("ReplicatedStorage").Weapons.M4A1.RangeModifier.Value = _G.Value
			game:GetService("ReplicatedStorage").Weapons.M4A1.Range.Value = _G.Value
		end
		if on == false then
			button.Text = "Range (False)"
			game:GetService("ReplicatedStorage").Weapons.M4A1.RangeModifier.Value = 98
			game:GetService("ReplicatedStorage").Weapons.M4A1.Range.Value = 800
		end
	end)
end)

