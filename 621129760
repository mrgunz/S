AltClickDelete.Name = "Alt Click Delete"
AltClickDelete.Parent = _2ndFrame
AltClickDelete.BackgroundColor3 = Color3.fromRGB(60, 60, 60)
AltClickDelete.BorderSizePixel = 0
AltClickDelete.Position = UDim2.new(0.0470629856, 0, 0.398101866, 0)
AltClickDelete.Size = UDim2.new(0, 117, 0, 32)
AltClickDelete.Font = Enum.Font.SourceSans
AltClickDelete.Text = "Alt Click Delete"
AltClickDelete.TextColor3 = Color3.fromRGB(0, 0, 0)
AltClickDelete.TextSize = 14.000
AltClickDelete.MouseButton1Down:connect(function()
	local Player = game:GetService("Players").LocalPlayer
	local Mouse = Player:GetMouse()

	Mouse.Button1Down:connect(function()
		if not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.LeftAlt) and not game:GetService("UserInputService"):IsKeyDown(Enum.KeyCode.RightAlt)then return end
		if not Mouse.Target then return end
		Mouse.Target:Destroy()
	end)
end)

